{
  "name": "@fit-booking/server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsup src/index.ts",
    "coverage": "DB_TYPE=pg-mem vitest run --coverage",
    "dev": "tsx watch src",
    "dev:mem": "DB_TYPE=pg-mem tsx watch src",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "migration:create": "node src/database/cli 'migration:create src/database/migrations/$0'",
    "migration:generate": "node src/database/cli 'migration:generate src/database/migrations/$0 -d src/database/cli/dataSource.ts'",
    "migration:revert": "npm run typeorm migration:revert -- -d src/database/cli/dataSource.ts",
    "migration:run": "tsx ../node_modules/typeorm/cli.js migration:run -d ./src/database/cli/dataSource.ts",
    "migration:show": "npm run typeorm migration:show -- -d src/database/cli/dataSource.ts",
    "start": "node ./dist/index.js",
    "test": "npm run test:mem",
    "test:mem": "DB_TYPE=pg-mem vitest",
    "test:db": "vitest",
    "typecheck": "tsc --noEmit",
    "typeorm": "NODE_OPTIONS='-r tsconfig-paths/register' typeorm-ts-node-commonjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^8.1.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "http-status-code": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "superjson": "^2.2.1",
    "trpc-panel": "^1.3.4",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "zod": "^3.22.4",
    "zod-validation-error": "^3.0.2"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.19",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitest/coverage-v8": "^1.3.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "pg-mem": "^2.8.1",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.0"
  }
}
